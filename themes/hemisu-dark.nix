# Hemisu Dark Theme Color Definitions
# A balanced color scheme with carefully selected colors
# https://noahfrederick.com/log/hemisu-for-os-x-terminal.html

{
  # Core color palette - Backgrounds & Panels
  bg_primary = "#111111";      # Almost black background
  bg_secondary = "#444444";    # Dark gray for panels
  bg_tertiary = "#777777";     # Middle dark gray for depth

  # Text & UI
  fg_primary = "#EEEEEE";      # Almost white text
  fg_secondary = "#9FD3E6";    # Middle light blue for secondary text
  fg_dim = "#999999";          # Middle light gray for comments

  # Accent colors
  accent_primary = "#538192";   # Middle dark blue - primary accent
  accent_secondary = "#739200"; # Middle dark green - secondary accent
  accent_tertiary = "#D0B03C";  # Yellow - tertiary accent

  # Special purpose colors
  cursor = "#EEEEEE";          # Almost white cursor
  selection_bg = "#005F87";    # Dark blue selection
  selection_fg = "#EEEEEE";    # Almost white text on selection
  url = "#538192";             # Middle dark blue for URLs

  # Border colors
  border_active_1 = "#538192";  # Middle dark blue
  border_active_2 = "#739200";  # Middle dark green
  border_inactive = "#777777";  # Middle dark gray

  # Terminal color palette (16 colors)
  color0 = "#111111";   # Black
  color1 = "#FF0055";   # Middle dark pink (red)
  color2 = "#739200";   # Middle dark green
  color3 = "#5F5F00";   # Dark green (yellow)
  color4 = "#005F87";   # Dark blue
  color5 = "#63001C";   # Dark pink (magenta)
  color6 = "#538192";   # Middle dark blue (cyan)
  color7 = "#BBBBBB";   # Light gray (white)

  # Bright variants
  color8 = "#777777";   # Bright Black
  color9 = "#D65E76";   # Bright Red
  color10 = "#B1D631";  # Bright Green
  color11 = "#D0B03C";  # Bright Yellow
  color12 = "#9FD3E6";  # Bright Blue
  color13 = "#FFAFAF";  # Bright Magenta
  color14 = "#CBE4EE";  # Bright Cyan
  color15 = "#FFFFFF";  # Bright White

  # Wallpapers
  wallpapers = [];
}
