# Smyck Theme Color Definitions
# A pleasant color scheme for terminals and text editors
# https://color.smyck.org/

{
  # Core color palette - Backgrounds & Panels
  bg_primary = "#282828";      # Dark gray background
  bg_secondary = "#3B3B3B";    # Lighter gray for panels
  bg_tertiary = "#424242";     # Tertiary background for depth

  # Text & UI
  fg_primary = "#F7F7F7";      # Light gray text
  fg_secondary = "#96D9F1";    # Cyan for secondary text
  fg_dim = "#8F8F8F";          # Medium gray for comments

  # Accent colors
